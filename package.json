{
  "name": "reversi-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "eventemitter3": "3.1.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "^2.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "worker": "tsc -p tsconfig.worker.json",
    "build": "npm run worker && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy:now": "npm run build && now build -p -n reversi --target=production"
  },
  "devDependencies": {
    "@types/eventemitter3": "^2.0.2",
    "@types/jest": "^23.3.12",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
